{
	"name": "app",
	"private": true,
	"version": "1.0.0",
	"scripts": {
		"dev": "vite",
		"build": "vite build --watch",
		"preview": "npm run build & vite preview",
		"lint": "eslint . --fix --max-warnings=0",
		"lint:fix": "eslint --fix .",
		"format": "prettier --write \"**/*.{js,jsx,json,md,html,css,less,scss}\"",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"prepare": "husky install"
	},
	"dependencies": {
		"@babel/runtime": "^7.22.11",
		"@coreui/react": "^4.10.1",
		"@reduxjs/toolkit": "^1.9.5",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^14.0.0",
		"@toast-ui/react-chart": "^4.7.1",
		"@vitejs/plugin-react": "^4.0.4",
		"apexcharts": "^3.45.2",
		"axios": "^1.4.0",
		"babel-eslint": "^10.1.0",
		"bootstrap": "5.3.0",
		"buffer": "^6.0.3",
		"classnames": "^2.3.2",
		"crypto-js": "^4.1.1",
		"dotenv": "^16.4.5",
		"draft-js": "^0.11.7",
		"draftjs-to-html": "^0.9.1",
		"formik": "^2.4.3",
		"html-react-parser": "^5.0.6",
		"i18next": "^23.4.1",
		"json-2-csv": "^5.5.4",
		"lodash": "^4.17.21",
		"metismenujs": "^1.4.0",
		"moment": "^2.29.4",
		"object-to-formdata": "^4.5.1",
		"prismjs": "^1.29.0",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-apexcharts": "^1.4.1",
		"react-beautiful-dnd": "^13.1.1",
		"react-copy-to-clipboard": "^5.1.0",
		"react-csv": "^2.2.2",
		"react-d3-tree": "^3.6.1",
		"react-date-range": "^2.0.1",
		"react-datepicker": "^7.1.0",
		"react-dom": "^18.2.0",
		"react-draft-wysiwyg": "^1.15.0",
		"react-dropzone": "^14.2.3",
		"react-flatpickr": "^3.10.13",
		"react-i18next": "^13.0.3",
		"react-image-lightbox": "^5.1.4",
		"react-paginate": "^8.2.0",
		"react-phone-input-2": "^2.15.1",
		"react-redux": "^8.1.2",
		"react-router-dom": "^6.14.2",
		"react-scripts": "^5.0.1",
		"react-select": "^5.7.4",
		"react-simple-code-editor": "^0.13.1",
		"react-table": "^7.8.0",
		"react-uuid": "^1.0.3",
		"reactstrap": "^9.2.0",
		"recharts": "^2.7.2",
		"redux": "^4.2.1",
		"redux-saga": "^1.2.3",
		"reselect": "^4.1.8",
		"simplebar-react": "^3.2.4",
		"styled-components": "^6.0.6",
		"toastr": "^2.1.4",
		"tui-chart": "^3.11.3",
		"vite": "^4.4.8",
		"yup": "^1.2.0"
	},
	"devDependencies": {
		"@babel/plugin-transform-runtime": "^7.22.9",
		"@types/react": "^18.2.18",
		"@types/react-dom": "^18.2.7",
		"babel-core": "^6.0.20",
		"eslint": "^8.50.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^9.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^14.0.1",
		"prettier": "3.0.3",
		"sass": "^1.72.0"
	},
	"eslintConfig": {
		"extends": [
			"eslint:recommended",
			"plugin:react/recommended"
		],
		"babelOptions": {
			"presets": [
				"@babel/preset-react"
			]
		},
		"parser": "@babel/eslint-parser",
		"parserOptions": {
			"requireConfigFile": false,
			"ecmaVersion": 2020,
			"sourceType": "module",
			"ecmaFeatures": {
				"jsx": true
			}
		},
		"env": {
			"browser": true,
			"es6": true
		},
		"globals": {
			"$": true
		},
		"settings": {
			"react": {
				"version": "^17.0.1"
			}
		},
		"rules": {
			"semi": [
				"error",
				"never"
			]
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"resolutions": {
		"moment": "2.29.1",
		"react-error-overlay": "6.0.11"
	},
	"lint-staged": {
		"*.{js,jsx}": "eslint --cache --fix",
		"*.{js,jsx,json,md,html,css,less,scss}": "prettier --write",
		"*.js": "eslint --cache --fix"
	}
}
